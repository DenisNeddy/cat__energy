{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "declaration-no-important": true,
        "indentation": [2, {"ignore": ["inside-parens"]}]
    }
}
